{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
    "jsx": "react",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/moment"
    ],
    "lib": [
      "dom",
      "es2015",
      "es2016",
    ],
  },
  "files": [
    "./App.tsx"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}